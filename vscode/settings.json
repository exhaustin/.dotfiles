{
    "editor.renderWhitespace": "all",
    "explorer.autoReveal": false,
    "vim.handleKeys": {
        "<C-d>": false
    },
    "vim.normalModeKeyBindingsNonRecursive": [
        {
        "before": ["i"],
        "after": ["<Up>"]
        },
        {
        "before": ["k"],
        "after": ["<Down>"]
        },
        {
        "before": ["j"],
        "after": ["<Left>"]
        },
        {
        "before": ["l"],
        "after": ["<Right>"]
        },
        {
        "before": ["h"],
        "after": ["i"]
        },
        {
        "before": ["H"],
        "after": ["I"]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
        "before": ["i"],
        "after": ["<Up>"]
        },
        {
        "before": ["k"],
        "after": ["<Down>"]
        },
        {
        "before": ["j"],
        "after": ["<Left>"]
        },
        {
        "before": ["l"],
        "after": ["<Right>"]
        },
        {
        "before": ["h"],
        "after": ["i"]
        },
        {
        "before": ["H"],
        "after": ["I"]
        }
    ]
}

